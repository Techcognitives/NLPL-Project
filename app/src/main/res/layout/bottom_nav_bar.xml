<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/action_bar_constrain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/background">

    <ImageView
        android:id="@+id/ejdcnjeknsd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/nav_unselected_blue"
        android:paddingTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/fuhhiedsfcjb"
        app:layout_constraintEnd_toStartOf="@+id/dscufsd"
        app:layout_constraintStart_toEndOf="@+id/fuhefhueusi"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dashboard_logo" />

    <ImageView
        android:id="@+id/duyewudscknjds"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/nav_selected_blue"
        android:paddingTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/fhuedfrcjberd"
        app:layout_constraintEnd_toStartOf="@+id/fuhefhueusi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_logo" />

    <ImageView
        android:id="@+id/fuhefhueusi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@color/nav_unselected_blue"
        android:paddingTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/dhuejsfcb"
        app:layout_constraintEnd_toStartOf="@+id/ejdcnjeknsd"
        app:layout_constraintStart_toEndOf="@+id/duyewudscknjds"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/track_load_logo" />

    <ImageView
        android:id="@+id/dscufsd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:background="@color/nav_unselected_blue"
        android:paddingTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/dfuhcisdekcf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ejdcnjeknsd"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/track_logo" />

    <TextView
        android:id="@+id/fhuedfrcjberd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/nav_selected_blue"
        android:paddingBottom="10dp"
        android:text="Profile"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/duyewudscknjds"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/duyewudscknjds" />

    <TextView
        android:id="@+id/dhuejsfcb"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@color/nav_unselected_blue"
        android:paddingBottom="10dp"
        android:text="Post a Load"
        android:textAlignment="center"
        android:textColor="@color/dark_grey"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ejdcnjeknsd"
        app:layout_constraintStart_toEndOf="@+id/duyewudscknjds"
        app:layout_constraintTop_toBottomOf="@+id/fuhefhueusi" />

    <TextView
        android:id="@+id/fuhhiedsfcjb"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/nav_unselected_blue"
        android:paddingBottom="10dp"
        android:text="Dashboard"
        android:textAlignment="center"
        android:textColor="@color/dark_grey"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ejdcnjeknsd"
        app:layout_constraintStart_toStartOf="@+id/ejdcnjeknsd"
        app:layout_constraintTop_toBottomOf="@+id/ejdcnjeknsd" />

    <TextView
        android:id="@+id/dfuhcisdekcf"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:background="@color/nav_unselected_blue"
        android:paddingBottom="10dp"
        android:text="Track"
        android:textAlignment="center"
        android:textColor="@color/dark_grey"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/dscufsd"
        app:layout_constraintStart_toEndOf="@+id/ejdcnjeknsd"
        app:layout_constraintTop_toBottomOf="@+id/dscufsd" />
</androidx.constraintlayout.widget.ConstraintLayout>