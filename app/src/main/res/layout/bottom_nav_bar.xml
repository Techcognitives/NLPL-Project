<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/action_bar_constrain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_nav_sp_dashboard"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:background="@drawable/personal_details_buttons_active"
        android:backgroundTint="@color/white"
        android:onClick="onClickBottomNavigation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bottom_nav_trip"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/bottom_nav_bar_dashboard_underline"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/personal_details_buttons_active"
            android:backgroundTint="@color/light_black"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/ejdcnjeknsd"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ejdcnjeknsd"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingTop="5dp"
            app:layout_constraintBottom_toTopOf="@+id/fuhhiedsfcjb"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bottom_nav_bar_dashboard_underline"
            app:srcCompat="@drawable/dashboard_logo" />

        <TextView
            android:id="@+id/fuhhiedsfcjb"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:text="@string/Dashboard"
            android:textAlignment="center"
            android:textColor="@color/light_black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ejdcnjeknsd" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_nav_customer_dashboard"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@drawable/personal_details_buttons_active"
        android:backgroundTint="@color/light_white"
        android:onClick="onClickBottomNavigation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bottom_nav_track"
        app:layout_constraintStart_toEndOf="@+id/bottom_nav_trip"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/bottom_nav_bar_find_underline"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/personal_details_buttons_active"
            android:backgroundTint="@color/light_black"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/bottom_nav_profile_image_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/bottom_nav_profile_image_view"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingTop="5dp"
            app:layout_constraintBottom_toTopOf="@+id/bottom_nav_profile_text_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bottom_nav_bar_find_underline"
            app:srcCompat="@drawable/bottom_nav_search_small" />

        <TextView
            android:id="@+id/bottom_nav_profile_text_view"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:text="@string/Profile"
            android:textAlignment="center"
            android:textColor="@color/light_black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bottom_nav_profile_image_view" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_nav_track"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:background="@drawable/personal_details_buttons_active"
        android:backgroundTint="@color/light_white"
        android:onClick="onClickBottomNavigation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bottom_nav_customer_dashboard"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/bottom_nav_bar_track_underline"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/personal_details_buttons_active"
            android:backgroundTint="@color/light_black"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/imageView22"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:text="@string/Track"
            android:textAlignment="center"
            android:textColor="@color/light_black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView22" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingTop="5dp"
            app:layout_constraintBottom_toTopOf="@+id/textView19"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bottom_nav_bar_track_underline"
            app:srcCompat="@drawable/track_logo_black" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_nav_trip"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:onClick="onClickBottomNavigation"
        android:background="@drawable/personal_details_buttons_active"
        android:backgroundTint="@color/light_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bottom_nav_customer_dashboard"
        app:layout_constraintStart_toEndOf="@+id/bottom_nav_sp_dashboard"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/bottom_nav_bar_trip_underline"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/personal_details_buttons_active"
            android:backgroundTint="@color/light_black"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/imageView23"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:text="Trips"
            android:textAlignment="center"
            android:textColor="@color/light_black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView23" />

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingTop="5dp"
            app:layout_constraintBottom_toTopOf="@+id/textView20"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bottom_nav_bar_trip_underline"
            app:srcCompat="@drawable/black_truck_small" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>