                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/post_a_load_select_city_pick_up"
                    app:layout_constraintEnd_toStartOf="@+id/post_a_load_select_state_pick_up"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/post_a_load_select_city_pick_up"
                    app:layout_constraintVertical_bias="1.0" />

                <TextView
