                    android:hint="@string/inrRs"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:paddingTop="7dp"
                    android:paddingEnd="2dp"
                    android:paddingBottom="7dp"
                    android:textColor="@color/green"
                    android:textSize="12dp"
                    app:layout_constraintBottom_toTopOf="@+id/textView65"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/textView63"
                    app:layout_constraintTop_toTopOf="parent" />

                <RadioButton
                    android:id="@+id/dialog_accept_bid_radio_btn_yes"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginEnd="5dp"
                    android:buttonTint="@color/radioBlue"
                    android:text="@string/yes_small"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/textView65"
                    app:layout_constraintEnd_toStartOf="@+id/dialog_accept_bid_radio_btn_no"
                    app:layout_constraintStart_toEndOf="@+id/textView65"
                    app:layout_constraintTop_toTopOf="@+id/textView65" />

                <RadioButton
